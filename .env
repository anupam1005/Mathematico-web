# API Configuration
VITE_API_URL=https://mathematico-backend-new.vercel.app/api/v1
VITE_API_TIMEOUT=10000

# Token Configuration
VITE_TOKEN_REFRESH_INTERVAL=840000

# App Configuration
VITE_APP_NAME=Mathematico
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_MODE=false

# Backend Configuration
VITE_BACKEND_URL=https://mathematico-backend-new.vercel.app
VITE_BACKEND_HEALTH_CHECK=/api/v1/health

# Frontend Configuration
VITE_FRONTEND_URL=https://mathematico-frontend.vercel.app
VITE_FRONTEND_PORT=443

# Build Configuration
VITE_BUILD_MODE=production

# Security Configuration
VITE_ENABLE_HTTPS=true
VITE_ENABLE_CORS=true

# Production Configuration
VITE_APP_ENV=production
VITE_APP_DOMAIN=mathematico-frontend.vercel.app

# Vercel Configuration
VITE_VERCEL=1
